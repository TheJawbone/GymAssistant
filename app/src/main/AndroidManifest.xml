<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dev.gymassistantv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gym_assistant_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseExerciseActivity"></activity>
        <activity android:name=".ProgressActivity"></activity>
        <activity android:name=".SetActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".WorkoutHistoryActivity"></activity>
    </application>

</manifest>